{
  "reg":{
    "name": "input[ng-model='registerUser.name']",
    "email": "input[ng-model='registerUser.email']",
    "pass": "input[ng-model='registerUser.password']",
    "passconfirm": "input[ng-model='passwordConfirm']"
  },
  "log":{
    "name": "input[ng-model='login.email']",
    "pass": "input[ng-model='login.password']"
  },
  "register": "a[ui-sref=start-dojo-wizard]",
  "login": "a[ui-sref=login]",
  "logout": "span[ng-click='logout()']",
  "dojoclick": "a[ng-click='viewDojo(dojo)']",
  "div": {
    "ng": "div[class='ng-binding']"
  }
}